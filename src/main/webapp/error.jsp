<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<style>
    /* Apply the color theme */
    body {
        background-color: #424242; /* Charcoal Gray for body background */
        color: #FFFFFF; /* White for primary text color */
        font-family: Arial, sans-serif;
        margin: 0;
        padding: 0;
        display: flex;
        justify-content: center;
        align-items: center;
        height: 100vh;
    }
    h3 {
        border: 2px solid #FFD700; /* Royal Gold for borders */
        color: #FFD700; /* Royal Gold for highlight text */
        padding: 15px;
        background-color: #212121; /* Deep Black for form background */
        text-align: center;
        border-radius: 10px;
    }
    button {
        background-color: #B71C1C; /* Rich Maroon for button background */
        color: #FFFFFF; /* White for button text */
        border: none;
        padding: 10px 20px;
        border-radius: 5px;
        cursor: pointer;
    }
    button:hover {
        background-color: #FFD700; /* Royal Gold for hover effect */
        color: #212121; /* Deep Black for hover text */
    }
</style>
</head>
<body>
    <h3>You can only add the menu from the same restaurant</h3>
</body>
</html>
